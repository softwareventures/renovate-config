{
  "name": "@softwareventures/renovate-config",
  "version": "0.0.0-development",
  "description": "Standard renovate configuration for Software Ventures Limited",
  "keywords": [
    "renovate"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/renovate-config",
  "bugs": "https://github.com/softwareventures/renovate-config/issues",
  "repository": "github:softwareventures/renovate-config",
  "license": "ISC",
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "renovate-config-validator default.json app.json lib.json"
  },
  "sideEffects": false,
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "devDependencies": {
    "@softwareventures/semantic-release-config": "3.0.0",
    "cz-conventional-changelog": "3.3.0",
    "re2": "1.17.7",
    "renovate": "34.29.2",
    "semantic-release": "19.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  },
  "renovate-config": {
    "app": {
      "extends": "./app.json"
    },
    "common": {
      "extends": "./common.json"
    },
    "common-app": {
      "extends": "./common-app.json"
    },
    "common-lib": {
      "extends": "./common-lib.json"
    },
    "default": {
      "extends": "./default.json"
    },
    "js-app": {
      "extends": "./js-app.json"
    },
    "js-lib": {
      "extends": "./js-lib.json"
    },
    "lib": {
      "extends": "./lib.json"
    },
    "pinAllExceptPeerDependencies": {
      "extends": "./pinAllExceptPeerDependencies.json"
    },
    "pinOnlyDevDependencies": {
      "extends": "./pinOnlyDevDependencies.json"
    },
    "rust-app": {
      "extends": "./rust-app.json"
    },
    "rust-lib": {
      "extends": "./rust-lib.json"
    }
  }
}
